<launch>
    <!-- Make ROS use simulator time instead of current clock time -->
    <param name="/use_sim_time" value="true" />

    <!-- Startup the Stage simulator on the 2nd floor -->
    <node pkg="stage" type="stageros" name="stageros" args="$(find precision_steering_tests)/harlie_empty_world.world" respawn="false" /> 
</launch>
